{
  "name": "strfy",
  "version": "0.0.3",
  "scripts": {
    "test": "ava",
    "compile": "coffee -o ./ -cw coffee/",
    "watch": "npm run compile &ava -w",
    "cover": "nyc ava",
    "report": "nyc report --reporter=html",
    "update:snap": "ava --update-snapshots"
  },
  "keywords": [
    "console"
  ],
  "dependencies": {
    "lodash": "^4.17.11",
    "objelity": "^1.0.0",
    "opn": "^5.4.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "coffeescript": "^2.2.3",
    "htmlhint": "^0.10.1",
    "nyc": "^13.1.0",
    "testdouble": "^3.8.2"
  },
  "files": [
    "lib",
    "dependence",
    "template",
    "image",
    "LICENSE.txt"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mick-whats/strfy"
  },
  "bugs": {
    "url": "https://github.com/mick-whats/strfy/issues"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*.js"
    ],
    "cache": false,
    "failFast": false,
    "serial": false,
    "tap": false,
    "verbose": true,
    "color": true,
    "watch": false
  },
  "main": "lib/index.js",
  "license": "MIT",
  "author": "mick-whats"
}
